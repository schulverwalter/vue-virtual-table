{
    "name": "vue-virtual-table",
    "version": "0.2.22",
    "description": "vue table component with virtual dom",
    "author": "https://github.com/waningflow",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/waningflow/vue-virtual-table.git"
    },
    "keywords": [
        "vue",
        "virtual",
        "component",
        "table",
        "smooth",
        "easy"
    ],
    "main": "dist/vue-virtual-table.umd.js",
    "module": "dist/vue-virtual-table.esm.js",
    "unpkg": "dist/vue-virtual-table.min.js",
    "browser": {
        "./sfc": "src/vue-virtual-table.vue"
    },
    "files": [
        "dist/*",
        "src/*"
    ],
    "scripts": {
        "ins": "yarn --registry https://registry.yarnpkg.com",
        "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
        "build:umd": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format umd",
        "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
        "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org"
    },
    "dependencies": {
        "floating-vue": "^1.0.0-beta.17",
        "vue-observe-visibility": "^0.4.3",
        "vue-resize": "^1.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.4.3",
        "@rollup/plugin-node-resolve": "^8.1.0",
        "cross-env": "^5.2.0",
        "minimist": "^1.2.0",
        "sass": "^1.70.0",
        "rollup": "^1.32.1",
        "rollup-plugin-babel": "^4.3.2",
        "rollup-plugin-buble": "^0.19.6",
        "rollup-plugin-commonjs": "^9.2.1",
        "rollup-plugin-css-only": "^2.1.0",
        "rollup-plugin-replace": "^2.1.0",
        "rollup-plugin-terser": "^4.0.4",
        "rollup-plugin-vue": "^4.7.2",
        "sass-loader": "^7.1.0",
        "vue": "^2.6.8",
        "vue-template-compiler": "^2.6.8"
    }
}
